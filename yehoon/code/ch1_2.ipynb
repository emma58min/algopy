   "source": [
    "def find_min(a):\n",
    "    n = len(a)\n",
    "    min_val = a[0]\n",
    "    for i in range(1, n):\n",
    "        if a[i] < min_val:\n",
    "            min_val = a[i]\n",
    "    return min_val"
   ]
  },
